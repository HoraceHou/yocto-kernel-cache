+++ b/arch/arm64/boot/dts/freescale/fsl-ls1088a-rdb.dts
@@ -107,14 +107,6 @@
 	status = "okay";
 };
 
-&usb0 {
-	status = "okay";
-};
-
-&usb1 {
-	status = "okay";
-};
-
 &esdhc {
 	status = "okay";
 };
diff --git a/arch/arm64/boot/dts/freescale/fsl-ls1088a.dtsi b/arch/arm64/boot/dts/freescale/fsl-ls1088a.dtsi
index ed3cedd914f9..2a96cd37734a 100644
