From 9d567c996708846f4ff24b2aebf29f320550b7f9 Mon Sep 17 00:00:00 2001
From: Madalin Bucur <madalin.bucur@nxp.com>
Date: Tue, 6 Jun 2017 15:04:50 +0300
Subject: [PATCH 030/767] arm64: dts: add ethernet@9

Signed-off-by: Madalin Bucur <madalin.bucur@nxp.com>
[Xulin: Original patch taken from NXP LSDK-18.09.]
Signed-off-by: Xulin Sun <xulin.sun@windriver.com>
---
 arch/arm64/boot/dts/freescale/fsl-ls1046a-rdb-sdk.dts | 7 +++++++
 arch/arm64/boot/dts/freescale/qoriq-dpaa-eth.dtsi     | 2 +-
 2 files changed, 8 insertions(+), 1 deletion(-)

diff --git a/arch/arm64/boot/dts/freescale/fsl-ls1046a-rdb-sdk.dts b/arch/arm64/boot/dts/freescale/fsl-ls1046a-rdb-sdk.dts
index 39b6c616bf40..70198e8cb72b 100644
--- a/arch/arm64/boot/dts/freescale/fsl-ls1046a-rdb-sdk.dts
+++ b/arch/arm64/boot/dts/freescale/fsl-ls1046a-rdb-sdk.dts
@@ -64,6 +64,13 @@
 #include "qoriq-dpaa-eth.dtsi"
 };
 
+&fsldpaa {
+	ethernet@9 {
+		compatible = "fsl,dpa-ethernet";
+		fsl,fman-mac = <&enet7>;
+	};
+};
+
 &fman0 {
 	compatible = "fsl,fman", "simple-bus";
 };
diff --git a/arch/arm64/boot/dts/freescale/qoriq-dpaa-eth.dtsi b/arch/arm64/boot/dts/freescale/qoriq-dpaa-eth.dtsi
index 20c4c7517393..eb5af912b988 100644
--- a/arch/arm64/boot/dts/freescale/qoriq-dpaa-eth.dtsi
+++ b/arch/arm64/boot/dts/freescale/qoriq-dpaa-eth.dtsi
@@ -32,7 +32,7 @@
  * SOFTWARE, EVEN IF ADVISED OF THE POSSIBILITY OF SUCH DAMAGE.
  */
 
-fsl,dpaa {
+fsldpaa: fsl,dpaa {
 	compatible = "fsl,ls1043a-dpaa", "simple-bus", "fsl,dpaa";
 	ethernet@0 {
 		compatible = "fsl,dpa-ethernet";
-- 
2.17.0

