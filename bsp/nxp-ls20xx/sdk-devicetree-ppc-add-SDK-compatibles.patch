+&bportals {
+	bman-portal@0 {
+		cell-index = <0>;
+	};
+
+	bman-portal@4000 {
+		cell-index = <1>;
+	};
+
+	bman-portal@8000 {
+		cell-index = <2>;
+	};
+
+	bman-portal@c000 {
+		cell-index = <3>;
+	};
+
+	bman-portal@10000 {
+		cell-index = <4>;
+	};
+
+	bman-portal@14000 {
+		cell-index = <5>;
+	};
+
+	bman-portal@18000 {
+		cell-index = <6>;
+	};
+
+	bman-portal@1c000 {
+		cell-index = <7>;
+	};
+
+	bman-portal@20000 {
+		cell-index = <8>;
+	};
+
+	bman-portal@24000 {
+		cell-index = <9>;
+	};
+
+	bman-bpids@0 {
+		compatible = "fsl,bpid-range";
+		fsl,bpid-range = <32 32>;
+	};
+};
