From aa435264dd02a6df64da713b885f1501b41de1bc Mon Sep 17 00:00:00 2001
From: Camelia Groza <camelia.groza@nxp.com>
Date: Thu, 29 Mar 2018 11:44:24 +0300
Subject: [PATCH 430/767] sdk: dts: ppc: add the PCD nodes for the second FMan

Signed-off-by: Camelia Groza <camelia.groza@nxp.com>
[Xulin: Original patch taken from NXP LSDK-18.09.]
Signed-off-by: Xulin Sun <xulin.sun@windriver.com>
---
 .../boot/dts/fsl/qoriq-fman3-1-sdk.dtsi       | 30 +++++++++++++++++++
 1 file changed, 30 insertions(+)

diff --git a/arch/powerpc/boot/dts/fsl/qoriq-fman3-1-sdk.dtsi b/arch/powerpc/boot/dts/fsl/qoriq-fman3-1-sdk.dtsi
index b75156e4dd5a..08971012368c 100644
--- a/arch/powerpc/boot/dts/fsl/qoriq-fman3-1-sdk.dtsi
+++ b/arch/powerpc/boot/dts/fsl/qoriq-fman3-1-sdk.dtsi
@@ -57,4 +57,34 @@
 	port@87000 {
 		fsl,qman-channel-id = <0x82e>;
 	};
+
+	policer@c0000 {
+		compatible = "fsl,fman-policer";
+		reg = <0xc0000 0x1000>;
+	};
+
+	keygen@c1000 {
+		compatible = "fsl,fman-keygen";
+		reg = <0xc1000 0x1000>;
+	};
+
+	dma@c2000 {
+		compatible = "fsl,fman-dma";
+		reg = <0xc2000 0x1000>;
+	};
+
+	fpm@c3000 {
+		compatible = "fsl,fman-fpm";
+		reg = <0xc3000 0x1000>;
+	};
+
+	parser@c7000 {
+		compatible = "fsl,fman-parser";
+		reg = <0xc7000 0x1000>;
+	};
+
+	vsps@dc000 {
+		compatible = "fsl,fman-vsps";
+		reg = <0xdc000 0x1000>;
+	};
 };
