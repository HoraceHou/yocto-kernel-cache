@@ -101,10 +101,15 @@
 		};
 		nand_pins: nand-pins {
 			marvell,pins =
-			"mpp17", "mpp18", "mpp19", "mpp20", "mpp21", "mpp22",
-			"mpp23", "mpp24", "mpp25", "mpp26", "mpp27";
+			"mpp15", "mpp16", "mpp17", "mpp18", "mpp19",
+			"mpp20", "mpp21", "mpp22", "mpp23", "mpp24",
+			"mpp25", "mpp26", "mpp27";
 			marvell,function = "dev";
 		};
+		nand_rb: nand-rb {
+			marvell,pins = "mpp13";
+			marvell,function = "nf";
+		};
 	};
 };
 
