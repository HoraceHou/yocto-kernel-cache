+++ b/drivers/usb/host/dwc_otg/dwc_otg_driver.c
@@ -241,9 +241,9 @@ static struct dwc_otg_driver_module_params dwc_otg_module_params = {
 };
 
 //Global variable to switch the fiq fix on or off
-bool fiq_enable = 1;
