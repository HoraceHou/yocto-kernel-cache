From 55b483e34eb693250e470522a74fe56674e00c3e Mon Sep 17 00:00:00 2001
From: Jiang Lu <lu.jiang@windriver.com>
Date: Thu, 6 Jun 2019 12:08:01 +0800
Subject: [PATCH 3/8] nvmem: imx-ocotp: Add imx7d soc into supporting list

The driver also support imx7d soc, add it into supporting list.

Signed-off-by: Jiang Lu <lu.jiang@windriver.com>
---
 drivers/nvmem/Kconfig | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/drivers/nvmem/Kconfig b/drivers/nvmem/Kconfig
index c7f5ca8dd2fd..966cafcceca6 100644
--- a/drivers/nvmem/Kconfig
+++ b/drivers/nvmem/Kconfig
@@ -26,7 +26,7 @@ config NVMEM_IMX_IIM
 
 config NVMEM_IMX_OCOTP
 	tristate "i.MX6 On-Chip OTP Controller support"
-	depends on SOC_IMX6 || ARCH_MXC_ARM64 || COMPILE_TEST
+	depends on SOC_IMX6 || SOC_IMX7D || ARCH_MXC_ARM64 || COMPILE_TEST
 	depends on HAS_IOMEM
 	help
 	  This is a driver for the On-Chip OTP Controller (OCOTP) available on
-- 
