-	unsigned int raw_data_index = 0;
-	int idx;
-
-	descriptor->header.type = DESCRIPTOR_TYPE_ESTABLISHED_TIMINGS_3;
-	descriptor->header.tag = 0xF7;
-	descriptor->version = raw_data[raw_data_index + 5];
