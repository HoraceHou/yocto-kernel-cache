-		int size = va_arg(vl, int);
-		if (size > 0) {
-			internal_itobe(va_arg(vl, int), dest, size);
-			dest += size;
-			len += size;;
-		} else {
-			memcpy((void *)dest, va_arg(vl, void *), -size);
-			dest -= size;
-			len -= size;
-		}
-	}
-	return len;
-}
