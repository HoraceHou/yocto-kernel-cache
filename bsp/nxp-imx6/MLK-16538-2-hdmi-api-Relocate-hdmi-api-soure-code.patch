+		reg_val = Afe_read(state, 0xC009);
+		reg_val &= 0x00FF;
+		reg_val |= 0x1200;
+		Afe_write(state, 0xC009, reg_val);
+
+		switch (clk_ratio) {
+		case CLK_RATIO_1_1:
