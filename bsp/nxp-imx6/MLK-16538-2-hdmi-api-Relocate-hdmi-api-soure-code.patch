+
+		/* register CMN_PLL0_VCOCAL_INIT_TMR */
+		Afe_write(state, 0x0084, 0x0064);
+		/* register CMN_PLL0_VCOCAL_ITER_TMR */
+		Afe_write(state, 0x0085, 0x000A);
