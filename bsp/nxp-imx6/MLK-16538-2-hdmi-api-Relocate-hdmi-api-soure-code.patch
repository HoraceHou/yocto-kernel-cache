-
-	if (bytes > sizeof(ret)) {
-		printk
-		    ("Warning. Read request for payload larger then supported.\n");
-		bytes = sizeof(ret);
-
-	}
-
-	for (i = 0; i < bytes; ++i) {
-		ret <<= 8;
-		ret |= (unsigned int)src[i];
-	}
-
-	return ret;
-}
