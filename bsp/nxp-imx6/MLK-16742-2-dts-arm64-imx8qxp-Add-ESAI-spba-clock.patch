From 2ab07b13ae280a7541c71aef2734c6b38f735be5 Mon Sep 17 00:00:00 2001
From: Viorel Suman <viorel.suman@nxp.com>
Date: Mon, 6 Nov 2017 15:05:14 +0200
Subject: [PATCH 2729/5242] MLK-16742-2: dts: arm64: imx8qxp: Add ESAI spba
 clock.

commit  b455f840fbc0cfdafa1a30ae727171ce4ac38260 from
https://source.codeaurora.org/external/imx/linux-imx.git

Add missing ESAI spba clock.

Signed-off-by: Viorel Suman <viorel.suman@nxp.com>
Reviewed-by: Shengjiu Wang <shengjiu.wang@nxp.com>
Signed-off-by: Meng Li <Meng.Li@windriver.com>
---
 arch/arm64/boot/dts/freescale/fsl-imx8qxp.dtsi |    5 +++--
 1 file changed, 3 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/freescale/fsl-imx8qxp.dtsi b/arch/arm64/boot/dts/freescale/fsl-imx8qxp.dtsi
index 33d5e46..41f3948 100644
--- a/arch/arm64/boot/dts/freescale/fsl-imx8qxp.dtsi
+++ b/arch/arm64/boot/dts/freescale/fsl-imx8qxp.dtsi
@@ -2068,8 +2068,9 @@
 		interrupts = <GIC_SPI 409 IRQ_TYPE_LEVEL_HIGH>;
 		clocks = <&clk IMX8QXP_AUD_ESAI_0_IPG>,
 			<&clk IMX8QXP_AUD_ESAI_0_EXTAL_IPG>,
-			<&clk IMX8QXP_AUD_ESAI_0_IPG>;
-		clock-names = "core", "extal", "fsys";
+			<&clk IMX8QXP_AUD_ESAI_0_IPG>,
+			<&clk IMX8QXP_CLK_DUMMY>;
+		clock-names = "core", "extal", "fsys", "spba";
 		dmas = <&edma2 6 0 1>, <&edma2 7 0 0>;
 		dma-names = "rx", "tx";
 		power-domains = <&pd_esai0>;
-- 
