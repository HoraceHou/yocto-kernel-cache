From ff124c080107d36ccaa69e67ae06304216153032 Mon Sep 17 00:00:00 2001
From: Xianzhong <xianzhong.li@nxp.com>
Date: Fri, 31 Aug 2018 01:39:32 +0800
Subject: [PATCH 4483/5242] MGS-4192 dts: gpu: add board compatible string

commit  ca942491390877f7f724db9bf800bd2b8763204a from
https://source.codeaurora.org/external/imx/linux-imx.git

add board compatible string for imx7ulp and m845s

Signed-off-by: Xianzhong <xianzhong.li@nxp.com>
Signed-off-by: Meng Li <Meng.Li@windriver.com>
---
 arch/arm/boot/dts/imx7ulp.dtsi                |    2 +-
 arch/arm64/boot/dts/freescale/fsl-imx8mm.dtsi |    2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm/boot/dts/imx7ulp.dtsi b/arch/arm/boot/dts/imx7ulp.dtsi
index b173508..fe6c657 100644
--- a/arch/arm/boot/dts/imx7ulp.dtsi
+++ b/arch/arm/boot/dts/imx7ulp.dtsi
@@ -654,7 +654,7 @@
 		};
 
 		gpu: gpu@41800000 {
-			compatible = "fsl,imx6q-gpu";
+			compatible = "fsl,imx7ulp-gpu", "fsl,imx6q-gpu";
 			reg = <0x41800000 0x80000>, <0x41880000 0x80000>,
 				<0x60000000 0x40000000>, <0x0 0x4000000>;
 			reg-names = "iobase_3d", "iobase_2d",
diff --git a/arch/arm64/boot/dts/freescale/fsl-imx8mm.dtsi b/arch/arm64/boot/dts/freescale/fsl-imx8mm.dtsi
index c6ffd89..ce6a335 100755
--- a/arch/arm64/boot/dts/freescale/fsl-imx8mm.dtsi
+++ b/arch/arm64/boot/dts/freescale/fsl-imx8mm.dtsi
@@ -1151,7 +1151,7 @@
 	};
 
 	gpu: gpu@38000000 {
-		compatible ="fsl,imx6q-gpu";
+		compatible ="fsl,imx8mm-gpu", "fsl,imx6q-gpu";
 		reg = <0x0 0x38000000 0x0 0x8000>, <0x0 0x38008000 0x0 0x8000>,
                         <0x0 0x40000000 0x0 0x80000000>, <0x0 0x0 0x0 0x8000000>;
 		reg-names = "iobase_3d", "iobase_2d",
-- 
