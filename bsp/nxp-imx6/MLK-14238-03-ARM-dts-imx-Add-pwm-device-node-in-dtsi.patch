From 2cd4407bdfb25f36f1d52227430bbb980ebd8d61 Mon Sep 17 00:00:00 2001
From: Bai Ping <ping.bai@nxp.com>
Date: Tue, 21 Mar 2017 11:18:25 +0800
Subject: [PATCH 1637/5242] MLK-14238-03 ARM: dts: imx: Add pwm device node in
 dtsi

commit  1e047576d6a4a4d3d4c045355890097588e84b5c from
https://source.codeaurora.org/external/imx/linux-imx.git

Add pwm device node in dtsi file.

Signed-off-by: Bai Ping <ping.bai@nxp.com>
Signed-off-by: Meng Li <Meng.Li@windriver.com>
---
 arch/arm/boot/dts/imx7ulp.dtsi |   11 +++++++++++
 1 file changed, 11 insertions(+)

diff --git a/arch/arm/boot/dts/imx7ulp.dtsi b/arch/arm/boot/dts/imx7ulp.dtsi
index d76040e..afde8040 100644
--- a/arch/arm/boot/dts/imx7ulp.dtsi
+++ b/arch/arm/boot/dts/imx7ulp.dtsi
@@ -10,6 +10,7 @@
 #include <dt-bindings/clock/imx7ulp-clock.h>
 #include <dt-bindings/interrupt-controller/arm-gic.h>
 #include <dt-bindings/gpio/gpio.h>
+#include <dt-bindings/pwm/pwm.h>
 #include "skeleton.dtsi"
 #include "imx7ulp-pinfunc.h"
 
@@ -194,6 +195,16 @@
 			status = "disabled";
 		};
 
+		pwm0: tpm@40250000 {
+			compatible = "nxp,tpm-pwm";
+			reg = <0x40250000 0x1000>;
+			nxp,pwm-number = <6>;
+			assigned-clocks = <&clks IMX7ULP_CLK_LPTPM4>;
+			assigned-clock-parents = <&clks IMX7ULP_CLK_OSC>;
+			clocks = <&clks IMX7ULP_CLK_LPTPM4>;
+			#pwm-cells = <2>;
+		};
+
 		tpm5: tpm@40260000 {
 			compatible = "fsl,imx7ulp-tpm";
 			reg = <0x40260000 0x1000>;
-- 
