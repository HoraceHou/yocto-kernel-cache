  * it under the terms of the GNU General Public License version 2 as
@@ -552,6 +553,12 @@
 			status = "disabled";
 		};
 
+		ocotp: ocotp@410a6000 {
+			compatible = "fsl,imx7ulp-ocotp";
+			reg = <0x410a6000 0x4000>;
+			clocks = <&clks IMX7ULP_CLK_DUMMY>;
+		};
+
 		gpu: gpu@41800000 {
 			compatible = "fsl,imx6q-gpu";
