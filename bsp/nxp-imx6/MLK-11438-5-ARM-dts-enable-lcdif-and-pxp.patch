From c9efc5b1872c6e80b0fea025fbc9b3bb1ed065f7 Mon Sep 17 00:00:00 2001
From: Robby Cai <r63905@freescale.com>
Date: Fri, 28 Aug 2015 15:28:31 +0800
Subject: [PATCH 0629/5242] MLK-11438-5 ARM: dts: enable lcdif and pxp

commit  ab77464cb5cc185eed485a922eeafc6782626315 from
https://source.codeaurora.org/external/imx/linux-imx.git

Enabled lcdif and pxp as lpsr mode is supported.

Signed-off-by: Robby Cai <r63905@freescale.com>
Signed-off-by: Meng Li <Meng.Li@windriver.com>
---
 arch/arm/boot/dts/imx7d-12x12-lpddr3-arm2.dts |    2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm/boot/dts/imx7d-12x12-lpddr3-arm2.dts b/arch/arm/boot/dts/imx7d-12x12-lpddr3-arm2.dts
index 11a4c72..da81d59 100644
--- a/arch/arm/boot/dts/imx7d-12x12-lpddr3-arm2.dts
+++ b/arch/arm/boot/dts/imx7d-12x12-lpddr3-arm2.dts
@@ -847,7 +847,7 @@
 	pinctrl-1 = <&pinctrl_lcdif_dat
 		     &pinctrl_lcdif_ctrl>;
 	display = <&display0>;
-	status = "disabled";
+	status = "okay";
 
 	display0: display {
 		bits-per-pixel = <16>;
-- 
