From f16e0ba419350d83587b222d622f16c0c7dac34e Mon Sep 17 00:00:00 2001
From: Anson Huang <Anson.Huang@nxp.com>
Date: Fri, 4 May 2018 16:10:06 +0800
Subject: [PATCH 3729/5242] MLK-18205-11 arm64: Kconfig: add i.MX8MM support

commit  db01fcd928b08f172b1692cdd68ac4eee22046d5 from
https://source.codeaurora.org/external/imx/linux-imx.git

Add i.MX8MM SoC support.

Signed-off-by: Anson Huang <Anson.Huang@nxp.com>
Reviewed-by: Bai Ping <ping.bai@nxp.com>
Signed-off-by: Meng Li <Meng.Li@windriver.com>
---
 arch/arm64/Kconfig.platforms |   13 +++++++++++++
 1 file changed, 13 insertions(+)

diff --git a/arch/arm64/Kconfig.platforms b/arch/arm64/Kconfig.platforms
index e097762..f4ff97b 100644
--- a/arch/arm64/Kconfig.platforms
+++ b/arch/arm64/Kconfig.platforms
@@ -340,4 +340,17 @@ config ARCH_FSL_IMX8MQ
 	help
 	  This enables support for Freescale i.MX8MQ SOC.
 
+config ARCH_FSL_IMX8MM
+	bool "Freescale i.MX8MM SOC"
+	select ARCH_MXC
+	select SERIAL_IMX
+	select PINCTRL
+	select PINCTRL_IMX
+	select PINCTRL_IMX8MM
+	select PM
+	select SOC_BUS
+	select KEYBOARD_SNVS_PWRKEY
+	help
+	  This enables support for Freescale i.MX8MM SOC.
+
 endmenu
