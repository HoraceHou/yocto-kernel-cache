From 7caa919e4bfc500e77334c6413ce7c8a4584fd83 Mon Sep 17 00:00:00 2001
From: Anson Huang <b20788@freescale.com>
Date: Thu, 4 Dec 2014 10:06:52 +0800
Subject: [PATCH 0396/5242] MLK-11488-6 arm: dts: imx6sx: add mu support

commit  93448db436e1761f26d32b6d7b49459bb8555c6a from
https://source.codeaurora.org/external/imx/linux-imx.git

add MU support for i.MX6SX.

Signed-off-by: Anson Huang <b20788@freescale.com>
Signed-off-by: Meng Li <Meng.Li@windriver.com>
---
 arch/arm/boot/dts/imx6sx.dtsi |    7 +++++++
 1 file changed, 7 insertions(+)

diff --git a/arch/arm/boot/dts/imx6sx.dtsi b/arch/arm/boot/dts/imx6sx.dtsi
index ccf4ac0..62b4595 100644
--- a/arch/arm/boot/dts/imx6sx.dtsi
+++ b/arch/arm/boot/dts/imx6sx.dtsi
@@ -1354,6 +1354,13 @@
 				status = "okay";
 			};
 
+			mu: mu@2294000 { /* mu */
+				compatible = "fsl,imx6sx-mu";
+				reg = <0x02294000 0x4000>;
+				interrupts = <0 90 0x04>;
+				status = "okay";
+			};
+
 			uart6: serial@22a0000 {
 				compatible = "fsl,imx6sx-uart",
 					     "fsl,imx6q-uart", "fsl,imx21-uart";
-- 
