From 5c76e79175e60f1d1fb777a32a28aa61a0614e6a Mon Sep 17 00:00:00 2001
From: Haibo Chen <haibo.chen@nxp.com>
Date: Tue, 11 Jul 2017 11:25:28 +0800
Subject: [PATCH 2119/5242] MLK-15949-3 ARM64: dts: fsl-imx8qm: correct USDHC
 per clock rate

commit  8afd23f987cc7eb01612269cc19bfc7e8926f2c1 from
https://source.codeaurora.org/external/imx/linux-imx.git

Set USDHC2/3 per clock's parent clock to 200MHz.

Signed-off-by: Haibo Chen <haibo.chen@nxp.com>
Signed-off-by: Meng Li <Meng.Li@windriver.com>
---
 arch/arm64/boot/dts/freescale/fsl-imx8qm.dtsi |    6 ++++--
 1 file changed, 4 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/freescale/fsl-imx8qm.dtsi b/arch/arm64/boot/dts/freescale/fsl-imx8qm.dtsi
index 68c9eb9..eef4da7 100644
--- a/arch/arm64/boot/dts/freescale/fsl-imx8qm.dtsi
+++ b/arch/arm64/boot/dts/freescale/fsl-imx8qm.dtsi
@@ -1991,7 +1991,8 @@
 			<&clk IMX8QM_SDHC1_CLK>,
 			<&clk IMX8QM_CLK_DUMMY>;
 		clock-names = "ipg", "per", "ahb";
-		assigned-clock-rates = <400000000>, <200000000>, <0>;
+		assigned-clocks = <&clk IMX8QM_SDHC1_DIV>;
+		assigned-clock-rates = <200000000>;
 		power-domains = <&pd_conn_sdch1>;
 		fsl,tuning-start-tap = <20>;
 		fsl,tuning-step= <2>;
@@ -2007,7 +2008,8 @@
 			<&clk IMX8QM_SDHC2_CLK>,
 			<&clk IMX8QM_CLK_DUMMY>;
 		clock-names = "ipg", "per", "ahb";
-		assigned-clock-rates = <400000000>, <200000000>, <0>;
+		assigned-clocks = <&clk IMX8QM_SDHC2_DIV>;
+		assigned-clock-rates = <200000000>;
 		power-domains = <&pd_conn_sdch2>;
 		status = "disabled";
 	};
-- 
