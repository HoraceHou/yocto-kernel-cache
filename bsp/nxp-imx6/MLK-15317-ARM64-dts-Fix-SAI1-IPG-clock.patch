From 7e44daa7de50cb243372417bab04d61ae72916cb Mon Sep 17 00:00:00 2001
From: Daniel Baluta <daniel.baluta@nxp.com>
Date: Mon, 10 Jul 2017 14:54:24 +0300
Subject: [PATCH 2092/5242] MLK-15317: ARM64: dts: Fix SAI1 IPG clock

commit  4cd38d731c69e09c34bdafe952dd752b5edf96e5 from
https://source.codeaurora.org/external/imx/linux-imx.git

Fixes: 293f390e81 ("ARM64: dts: Add SAI1 definition")
Reported-by: Viorel Suman <viorel.suman@nxp.com>
Signed-off-by: Daniel Baluta <daniel.baluta@nxp.com>
Reviewed-by: Viorel Suman <viorel.suman@nxp.com>
Signed-off-by: Meng Li <Meng.Li@windriver.com>
---
 arch/arm64/boot/dts/freescale/fsl-imx8qxp.dtsi |    2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm64/boot/dts/freescale/fsl-imx8qxp.dtsi b/arch/arm64/boot/dts/freescale/fsl-imx8qxp.dtsi
index de7f758..02edfd8 100644
--- a/arch/arm64/boot/dts/freescale/fsl-imx8qxp.dtsi
+++ b/arch/arm64/boot/dts/freescale/fsl-imx8qxp.dtsi
@@ -1387,7 +1387,7 @@
 		compatible = "fsl,imx8qm-sai";
 		reg = <0x0 0x59050000 0x0 0x10000>;
 		interrupts = <GIC_SPI 316 IRQ_TYPE_LEVEL_HIGH>;
-		clocks = <&clk IMX8QXP_AUD_SAI_0_IPG>,
+		clocks = <&clk IMX8QXP_AUD_SAI_1_IPG>,
 			<&clk IMX8QXP_CLK_DUMMY>,
 			<&clk IMX8QXP_AUD_SAI_1_MCLK>,
 			<&clk IMX8QXP_CLK_DUMMY>,
-- 
