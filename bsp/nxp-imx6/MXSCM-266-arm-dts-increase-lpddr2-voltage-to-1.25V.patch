From 8ecfa3cc4cc2bedf6a8bb0b54a7454a613e2f27e Mon Sep 17 00:00:00 2001
From: Juan Gutierrez <juan.gutierrez@nxp.com>
Date: Thu, 9 Mar 2017 17:49:36 -0600
Subject: [PATCH 1693/5242] MXSCM-266 arm: dts: increase lpddr2 voltage to
 1.25V

commit  39e8b54c85c961bd9f1704c274296dd2c05dbc6e from
https://source.codeaurora.org/external/imx/linux-imx.git

From testing the performance is better when the voltage
for lpddr2 is set to 1.25V instead of 1.2V.

Signed-off-by: Juan Gutierrez <juan.gutierrez@nxp.com>
Signed-off-by: Meng Li <Meng.Li@windriver.com>
---
 arch/arm/boot/dts/imx6dqscm-1gb-evb-fix-ldo.dts |    2 +-
 arch/arm/boot/dts/imx6dqscm-qwks-rev2.dtsi      |    2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm/boot/dts/imx6dqscm-1gb-evb-fix-ldo.dts b/arch/arm/boot/dts/imx6dqscm-1gb-evb-fix-ldo.dts
index 560f8ef..f0dcf7d 100644
--- a/arch/arm/boot/dts/imx6dqscm-1gb-evb-fix-ldo.dts
+++ b/arch/arm/boot/dts/imx6dqscm-1gb-evb-fix-ldo.dts
@@ -79,7 +79,7 @@
 
 			sw3a_reg: sw3a {
 				regulator-min-microvolt = <400000>;
-				regulator-max-microvolt = <1200000>;
+				regulator-max-microvolt = <1250000>;
 				regulator-boot-on;
 				regulator-always-on;
 			};
diff --git a/arch/arm/boot/dts/imx6dqscm-qwks-rev2.dtsi b/arch/arm/boot/dts/imx6dqscm-qwks-rev2.dtsi
index f631e66..e69302f 100644
--- a/arch/arm/boot/dts/imx6dqscm-qwks-rev2.dtsi
+++ b/arch/arm/boot/dts/imx6dqscm-qwks-rev2.dtsi
@@ -216,7 +216,7 @@
 
 			sw3a_reg: sw3a {
 				regulator-min-microvolt = <400000>;
-				regulator-max-microvolt = <1200000>;
+				regulator-max-microvolt = <1250000>;
 				regulator-boot-on;
 				regulator-always-on;
 			};
