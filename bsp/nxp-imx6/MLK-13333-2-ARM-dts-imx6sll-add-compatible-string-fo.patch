From 6752d09e59d2664f0a2a7fa4ae2b9b790c19aafe Mon Sep 17 00:00:00 2001
From: Anson Huang <Anson.Huang@nxp.com>
Date: Thu, 13 Oct 2016 18:23:09 +0800
Subject: [PATCH 1183/5242] MLK-13333-2 ARM: dts: imx6sll: add compatible
 string for iomuxc gpr

commit  ab3529170a839b88f0509378782490e49f42d49d from
https://source.codeaurora.org/external/imx/linux-imx.git

Add "fsl,imx6q-iomuxc-gpr" compatible string for i.MX6SLL.

Signed-off-by: Anson Huang <Anson.Huang@nxp.com>
Signed-off-by: Meng Li <Meng.Li@windriver.com>
---
 arch/arm/boot/dts/imx6sll.dtsi |    3 ++-
 1 file changed, 2 insertions(+), 1 deletion(-)

diff --git a/arch/arm/boot/dts/imx6sll.dtsi b/arch/arm/boot/dts/imx6sll.dtsi
index 3fb9ece..4a4a07f 100644
--- a/arch/arm/boot/dts/imx6sll.dtsi
+++ b/arch/arm/boot/dts/imx6sll.dtsi
@@ -578,7 +578,8 @@
 			};
 
 			gpr: iomuxc-gpr@020e4000 {
-				compatible = "fsl,imx6sll-iomuxc-gpr", "syscon";
+				compatible = "fsl,imx6sll-iomuxc-gpr",
+					     "fsl,imx6q-iomuxc-gpr", "syscon";
 				reg = <0x020e4000 0x4000>;
 			};
 
