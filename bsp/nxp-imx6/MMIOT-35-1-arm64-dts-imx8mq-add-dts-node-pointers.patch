From 8e0fb832ff513a989ca131db9972eee21d6fc846 Mon Sep 17 00:00:00 2001
From: Antoine Bouyer <antoine.bouyer@nxp.com>
Date: Thu, 7 Sep 2017 16:41:02 +0200
Subject: [PATCH 3826/5242] MMIOT-35-1 arm64: dts: imx8mq: add dts node
 pointers

commit  136c9bcc21099e22a6d8888b498a84720c97de79 from
https://source.codeaurora.org/external/imx/linux-imx.git

These pointers are required for drm dts

Signed-off-by: Antoine Bouyer <antoine.bouyer@nxp.com>
Signed-off-by: Meng Li <Meng.Li@windriver.com>
---
 arch/arm64/boot/dts/freescale/fsl-imx8mq.dtsi |    4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/freescale/fsl-imx8mq.dtsi b/arch/arm64/boot/dts/freescale/fsl-imx8mq.dtsi
index 062385c..c4e4164 100755
--- a/arch/arm64/boot/dts/freescale/fsl-imx8mq.dtsi
+++ b/arch/arm64/boot/dts/freescale/fsl-imx8mq.dtsi
@@ -70,7 +70,7 @@
 		reg = <0x00000000 0x40000000 0 0xc0000000>;
 	};
 
-	reserved-memory {
+	resmem: reserved-memory {
 		#address-cells = <2>;
 		#size-cells = <2>;
 		ranges;
@@ -1191,7 +1191,7 @@
 		status = "disabled";
 	};
 
-	imx_ion {
+	imx_ion: imx_ion {
 		compatible = "fsl,mxc-ion";
 		fsl,heap-id = <0>;
 	};
-- 
