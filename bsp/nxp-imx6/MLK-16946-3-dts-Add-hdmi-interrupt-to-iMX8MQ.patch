From cd3cf8dbd1957ca9d17f7581c468c89d59a8e20c Mon Sep 17 00:00:00 2001
From: Sandor Yu <Sandor.yu@nxp.com>
Date: Thu, 23 Nov 2017 12:24:23 +0800
Subject: [PATCH 2917/5242] MLK-16946-3: dts: Add hdmi interrupt to iMX8MQ

commit  d37ff7641740ff6764979bfcb525dcf7def7d8fb from
https://source.codeaurora.org/external/imx/linux-imx.git

Add hdmi interrupt to iMX8MQ dts.

Signed-off-by: Sandor Yu <Sandor.yu@nxp.com>
Signed-off-by: Meng Li <Meng.Li@windriver.com>
---
 arch/arm64/boot/dts/freescale/fsl-imx8mq.dtsi |    5 ++++-
 1 file changed, 4 insertions(+), 1 deletion(-)

diff --git a/arch/arm64/boot/dts/freescale/fsl-imx8mq.dtsi b/arch/arm64/boot/dts/freescale/fsl-imx8mq.dtsi
index 9c3b94a..e695347 100644
--- a/arch/arm64/boot/dts/freescale/fsl-imx8mq.dtsi
+++ b/arch/arm64/boot/dts/freescale/fsl-imx8mq.dtsi
@@ -574,7 +574,10 @@
 		compatible = "fsl,imx8mq-hdmi";
 		reg = <0x0 0x32c00000 0x0 0x33800>,	/* HDP registers */
 				<0x0 0x32e40000 0x0 0x40000>;   /* HDP SEC register */
-		interrupts = <GIC_SPI 16 IRQ_TYPE_LEVEL_HIGH>;
+		interrupts = <GIC_SPI 16 IRQ_TYPE_LEVEL_HIGH>,
+					<GIC_SPI 25 IRQ_TYPE_LEVEL_HIGH>;
+		interrupt-names = "plug_in", "plug_out";
+		interrupt-parent = <&gpc>;
 		status = "disabled";
 
 		port@0 {
-- 
