 	{ 1,		128,	1,	128,	1,	24,	},
 	{ 2,		192,	1,	192,	1,	32,	},
-	{ 2,		192,	1,	128,	1,	32,	},
-	{ 0, /* Prohibited setting */				},
+	{ 2,		192,	1,	168,	1,	32,	},
+	{ 2,		192,	1,	144,	1,	32,	},
 	{ 2,		192,	1,	192,	1,	32,	},
 };
 
