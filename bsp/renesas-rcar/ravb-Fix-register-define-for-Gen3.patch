+	RID1_RWD14	= 0x00004000,
+	RID1_RWD15	= 0x00008000,
+	RID1_RWD16	= 0x00010000,
+	RID1_RWD17	= 0x00020000,
+	RID1_RFWD	= 0x80000000,
+};
+
 /* RIE2 (R-Car Gen3 only) */
 enum RIE2_BIT {
 	RIE2_QFS0	= 0x00000001,
