Subject: [PATCH 071/706] arm64: dts: ls1028a: update to sim m166 for enetc

Signed-off-by: Alex Marginean <alexandru.marginean@freescale.com>
(cherry picked from commit a757e6b25762cbf9ccbd14b332a91e6b817274d4)
Signed-off-by: Zumeng Chen <zumeng.chen@windriver.com>
---
 arch/arm64/boot/dts/freescale/fsl-ls1028a.dtsi | 13 ++++++-------
 1 file changed, 6 insertions(+), 7 deletions(-)

diff --git a/arch/arm64/boot/dts/freescale/fsl-ls1028a.dtsi b/arch/arm64/boot/dts/freescale/fsl-ls1028a.dtsi
index 693f9ce3f274..4e68c95f0799 100755
--- a/arch/arm64/boot/dts/freescale/fsl-ls1028a.dtsi
+++ b/arch/arm64/boot/dts/freescale/fsl-ls1028a.dtsi
@@ -481,13 +481,12 @@
 			msi-parent = <&its>;
 			device_type = "pci";
 			bus-range = <0x0 0x0>;
-			ranges = <0x82000000 0x0 0x00000000  0x1 0xf8000000  0x0 0x0C0000     /* PF0-2, BAR0 - non-prefetchable memory */
-				  0x82000000 0x0 0x00000000  0x1 0xf8100000  0x0 0x060000     /* PF3-5, BAR0 - non-prefetchable memory */
-				  0xc2000000 0x0 0x00000000  0x1 0xf8160000  0x0 0x050000     /* PF0-5, BAR2 - prefetchable memory */
-				  0x82000000 0x0 0x00000000  0x1 0xf81c0000  0x0 0x020000     /* PF0, VF-BAR0 - non-prefetchable memory */
-				  0xc2000000 0x0 0x00000000  0x1 0xf81e0000  0x0 0x020000     /* PF0, VF-BAR2 - prefetchable memory */
-				  0x82000000 0x0 0x00000000  0x1 0xf8200000  0x0 0x020000     /* PF1, VF-BAR0 - non-prefetchable memory */
-				  0xc2000000 0x0 0x00000000  0x1 0xf8220000  0x0 0x020000     /* PF1, VF-BAR1 - prefetchable memory */
+			ranges = <0x82000000 0x0 0x00000000  0x1 0xf8000000  0x0 0x160000     /* PF0-6 BAR0 - non-prefetchable memory */
+				  0xc2000000 0x0 0x00000000  0x1 0xf8160000  0x0 0x070000     /* PF0-6, BAR2 - prefetchable memory */
+				  0x82000000 0x0 0x00000000  0x1 0xf81d0000  0x0 0x020000     /* PF0, VF-BAR0 - non-prefetchable memory */
+				  0xc2000000 0x0 0x00000000  0x1 0xf81f0000  0x0 0x020000     /* PF0, VF-BAR2 - prefetchable memory */
+				  0x82000000 0x0 0x00000000  0x1 0xf8210000  0x0 0x020000     /* PF1, VF-BAR0 - non-prefetchable memory */
+				  0xc2000000 0x0 0x00000000  0x1 0xf8230000  0x0 0x020000     /* PF1, VF-BAR1 - prefetchable memory */
 				  0x82000000 0x0 0x00000000  0x1 0xfc000000  0x0 0x400000>;   /* PF2/PF5, BAR4 - non-prefetchable memory */
 		};
 	};
