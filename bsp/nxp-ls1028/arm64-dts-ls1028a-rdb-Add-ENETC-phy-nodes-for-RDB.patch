From 594885af9ce9508e13c025f32a17bc83d2ce9ea7 Mon Sep 17 00:00:00 2001
From: Claudiu Manoil <claudiu.manoil@nxp.com>
Date: Mon, 3 Sep 2018 18:50:29 +0300
Subject: [PATCH 287/706] arm64: dts: ls1028a-rdb: Add ENETC phy nodes for RDB

Signed-off-by: Claudiu Manoil <claudiu.manoil@nxp.com>
(cherry picked from commit 57425a1290f135784b159fe67fa366a3a93db1f2)
Signed-off-by: Zumeng Chen <zumeng.chen@windriver.com>
---
 .../boot/dts/freescale/fsl-ls1028a-rdb.dts    | 33 +++++++++++++++++++
 1 file changed, 33 insertions(+)

diff --git a/arch/arm64/boot/dts/freescale/fsl-ls1028a-rdb.dts b/arch/arm64/boot/dts/freescale/fsl-ls1028a-rdb.dts
index e67df19154a5..ef7e94eb50e1 100755
--- a/arch/arm64/boot/dts/freescale/fsl-ls1028a-rdb.dts
+++ b/arch/arm64/boot/dts/freescale/fsl-ls1028a-rdb.dts
@@ -54,6 +54,7 @@
 	aliases {
 		serial0 = &duart0;
 		serial1 = &duart1;
+		enetc_port0_sgmii_phy0 = &sgmii_phy0;
 	};
 
 	chosen {
@@ -226,3 +227,35 @@
 &dp0 {
 	status = "okay";
 };
+
+&mdio0 {
+	sgmii_phy0: ethernet-phy@2 {
+		compatible = "ethernet-phy-id004d.d074";
+		reg = <0x2>;
+	};
+};
+
+&enetc_port0 {
+	phy-handle = <&sgmii_phy0>;
+	phy-connection-type = "sgmii";
+};
+
+&enetc_port1 {
+	status = "disabled";
+};
+
+&enetc_port2 {
+	phy-connection-type = "internal";
+	fixed-link {
+		speed = <1000>;
+		full-duplex;
+	};
+};
+
+&enetc_port3 {
+	phy-connection-type = "internal";
+	fixed-link {
+		speed = <1000>;
+		full-duplex;
+	};
+};
-- 
