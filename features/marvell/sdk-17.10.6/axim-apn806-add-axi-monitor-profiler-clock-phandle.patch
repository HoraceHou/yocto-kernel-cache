https://github.com/MarvellEmbeddedProcessors/linux-marvell.git

Change-Id: I2f661d7c08da567d60dc143e364266bdd8834578
Signed-off-by: Shadi Ammouri <shadi@marvell.com>
Reviewed-on: http://vgitil04.il.marvell.com:8080/31991
Tested-by: Star_Automation <star@marvell.com>
Reviewed-by: Hanna Hawa <hannah@marvell.com>
Signed-off-by: Meng Li <Meng.Li@windriver.com>
---
 arch/arm64/boot/dts/marvell/armada-ap806.dtsi |    8 ++++----
 1 file changed, 4 insertions(+), 4 deletions(-)

diff --git a/arch/arm64/boot/dts/marvell/armada-ap806.dtsi b/arch/arm64/boot/dts/marvell/armada-ap806.dtsi
index 4ec553b..9cea483 100644
--- a/arch/arm64/boot/dts/marvell/armada-ap806.dtsi
+++ b/arch/arm64/boot/dts/marvell/armada-ap806.dtsi
@@ -162,16 +162,16 @@
 			axim-ddr-rd@840000 {
 				compatible = "marvell,coresight-axim", "arm,primecell";
 				reg = <0x840000 0x1000>;
-				clocks = <&syscon 3>;
-				clock-names = "apb_pclk";
+				clocks = <&syscon 3>, <&syscon 5>;
+				clock-names = "apb_pclk", "hclk";
 				bus-width = <40>;
 			};
 
 			axim-ddr-wr@841000 {
 				compatible = "marvell,coresight-axim", "arm,primecell";
 				reg = <0x841000 0x1000>;
-				clocks = <&syscon 3>;
-				clock-names = "apb_pclk";
+				clocks = <&syscon 3>, <&syscon 5>;
+				clock-names = "apb_pclk", "hclk";
 				bus-width = <40>;
 			};
 
