+/dts-v1/;
+
+#include "armada-372x-rz.dtsi"
+
+/ {
+	model = "Marvell Armada 3720 Community Board";
+	compatible = "marvell,armada-3720-community", "marvell,armada3720", "marvell,armada3710";
+
+	chosen {
+		stdout-path = "serial0:115200n8";
+	};
+
+	memory {
+		device_type = "memory";
+		reg = <0x00000000 0x00000000 0x00000000 0x20000000>;
+	};
+
+	soc {
+		internal-regs {
+			uart0: serial@12000 {
+				status = "okay";
+			};
+
+			i2c0: i2c@11000 {
+				status = "okay";
+			};
+
+			i2c1: i2c@11080 {
+				status = "okay";
+			};
+
+			eth0: ethernet@30000 {
+				phy-mode = "rgmii-id";
+				status = "okay";
+				fixed-link {
+					speed = <1000>;
+					full-duplex;
+				};
+			};
+
+			usb3@58000 {
+				status = "okay";
+			};
+
+			usb@5e000 {
+				status = "okay";
+			};
+
+			sdhci1: sdhci@d0000 {
+				wp-inverted;
+				bus-width = <4>;
+				cd-gpios = <&gpio_nb 3 GPIO_ACTIVE_LOW>;
+				no-1-8-v;
+				status = "okay";
+
+				pinctrl-names = "default";
+				pinctrl-0 = <&sdio_pins &cd_pins>;
+			};
+
+			sata@e0000 {
+				status = "okay";
+			};
+		};
+	};
+};
