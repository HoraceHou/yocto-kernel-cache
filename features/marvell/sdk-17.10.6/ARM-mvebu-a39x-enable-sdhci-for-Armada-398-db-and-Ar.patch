From 76494bf07cec2f2df936612b3d0534095e9da7b3 Mon Sep 17 00:00:00 2001
From: Grzegorz Jaszczyk <jaz@semihalf.com>
Date: Wed, 1 Jun 2016 15:16:00 +0200
Subject: [PATCH 0266/1345] ARM: mvebu: a39x: enable sdhci for Armada-398-db
 and Armada-390-db boards

commit  4dd4666d4737c38b16c6967e41f4f01e801b8449 from
https://github.com/MarvellEmbeddedProcessors/linux-marvell.git

Change-Id: I5d2c05195a62db4217069814fab277a63bed9409
Signed-off-by: Grzegorz Jaszczyk <jaz@semihalf.com>
Reviewed-on: http://vgitil04.il.marvell.com:8080/30281
Tested-by: Star_Automation <star@marvell.com>
Reviewed-by: Lior Amsalem <alior@marvell.com>
Signed-off-by: Meng Li <Meng.Li@windriver.com>
---
 arch/arm/boot/dts/armada-390-db.dts |   10 ++++++++++
 arch/arm/boot/dts/armada-398-db.dts |    9 +++++++++
 2 files changed, 19 insertions(+)

diff --git a/arch/arm/boot/dts/armada-390-db.dts b/arch/arm/boot/dts/armada-390-db.dts
index 2235b92..61052b1 100644
--- a/arch/arm/boot/dts/armada-390-db.dts
+++ b/arch/arm/boot/dts/armada-390-db.dts
@@ -138,6 +138,15 @@
 				};
 			};
 
+			sdhci@d8000 {
+				clock-frequency = <200000000>;
+				broken-cd;
+				wp-inverted;
+				bus-width = <8>;
+				status = "okay";
+				no-1-8-v;
+			};
+
 			/* CON98 */
 			usb3@f8000 {
 				status = "okay";
@@ -148,6 +157,7 @@
 			pinctrl-0 = <&smi_nss_pins>;
 			pinctrl-names="default";
 		};
+
 		nss_complex {
 			status = "okay";
 			pinctrl-0 = <&smi_nss_pins>;
diff --git a/arch/arm/boot/dts/armada-398-db.dts b/arch/arm/boot/dts/armada-398-db.dts
index 39cae9e..372d240 100644
--- a/arch/arm/boot/dts/armada-398-db.dts
+++ b/arch/arm/boot/dts/armada-398-db.dts
@@ -84,6 +84,15 @@
 				status = "okay";
 			};
 
+			sdhci@d8000 {
+				clock-frequency = <200000000>;
+				broken-cd;
+				wp-inverted;
+				bus-width = <8>;
+				status = "okay";
+				no-1-8-v;
+			};
+
 			usb@58000 {
 				status = "okay";
 			};
