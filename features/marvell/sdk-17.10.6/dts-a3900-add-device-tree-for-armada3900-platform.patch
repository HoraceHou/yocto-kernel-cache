+/ {
+	model = "Marvell Armada 3900 AXIS Board";
+	compatible = "marvell,armada3900-db-axis", "marvell,armada7040",
+		     "marvell,armada-ap806-quad", "marvell,armada-ap806";
+
+	cpn-110-master {
+		config-space {
+			pinctrl@440000 {
+				cpm_xhci0_vbus_pins: cpm-xhci0-vbus-pins {
+					marvell,pins = "mpp7";
+					marvell,function = "gpio";
+				};
+				cpm_xhci1_vbus_pins: cpm-xhci1-vbus-pins {
+					marvell,pins = "mpp5";
+					marvell,function = "gpio";
+				};
+			};
+			usb3h0: usb3@500000 {
+				separated-phys-for-usb2-usb3;
+				phys = <&comphy0 1 COMPHY_USB3H0>;
+				phy-names = "usb3";
+			};
+		};
+
+		pcie@0x600000 {
+			phys = <&comphy0 0 COMPHY_PCIE0>;
+			phy-names = "pcie-phy0";
+		};
+	};
+
+	cpm_reg_usb3_vbus0: cpm_usb3_vbus@0 {
+		gpio = <&cpm_gpio0 7 GPIO_ACTIVE_HIGH>;
+	};
+	cpm_reg_usb3_vbus1: cpm_usb3_vbus@1 {
+		gpio = <&cpm_gpio0 5 GPIO_ACTIVE_HIGH>;
+	};
+};
